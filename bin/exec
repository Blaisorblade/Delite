#!/bin/bash

$JAVA_HOME/bin/java -server -d64 -XX:+UseCompressedOops -XX:+DoEscapeAnalysis -Xmx1396m -Xms512m -Xss128m -XX:+UseConcMarkSweepGC $JAVA_OPTS -cp $DELITE_HOME/runtime/target/scala_2.8.1/classes:$SCALA_HOME/lib/scala-library.jar:$SCALA_HOME/lib/scala-compiler.jar:$DELITE_HOME/runtime/lib_managed/scala_2.8.1/compile/* ppl.delite.runtime.Delite "$@"
