#!/bin/bash

$JAVA_HOME/bin/java -server -d64 -XX:+UseCompressedOops -XX:+DoEscapeAnalysis -Xmx16g -Xms16g $JAVA_OPTS -cp $HOME/delite/runtime/target/scala_2.8.1/classes:$HOME/scala/scala-2.8.1.final/lib/scala-library.jar:$HOME/scala/scala-2.8.1.final/lib/scala-compiler.jar ppl.delite.runtime.Delite "$@"
