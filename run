#!/bin/bash

export SCALA_NORMAL=~/scala-2.8.1.final
java -cp runtime/target/scala_2.8.1/classes:$SCALA_NORMAL/lib/scala-library.jar:$SCALA_NORMAL/lib/scala-compiler.jar -Xmx20g -Xms20g -Ddelite.profile.enabled=true -Ddelite.profile.immediateprint=false -Ddelite.profile.showstamp=false -Ddelite.threads=1 ppl.delite.runtime.Delite "$@"
