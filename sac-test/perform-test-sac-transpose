#!/bin/bash
echo Set classpath...
export CLASSPATH=$CLASSPATH:$DELITE/dsls/sac-staged/target/scala_2.9.x-virtualized-SNAPSHOT/classes/:$DELITE/framework/target/scala_2.9.x-virtualized-SNAPSHOT/classes:$LMS/target/scala_2.9.x-virtualized-SNAPSHOT/classes:.
#echo Compile...
$SCALA_VIRTUALIZED/scalac Transpose.scala

echo Stage...
$SCALA_VIRTUALIZED/scala -Ddelite.home.dir=$DELITE -DStagedSAC.blocks=true TransposeRunner #-Dlms.verbosity=2
echo Execute...
java -Xmx8g -server -cp $DELITE/runtime/target/$VANILLA_VERSION/classes:$SCALA_VANILLA/../lib/scala-library.jar:$SCALA_VANILLA/../lib/scala-compiler.jar -Ddelite.runs=10 ppl.delite.runtime.Delite out.deg

echo Stage...
$SCALA_VIRTUALIZED/scala -Ddelite.home.dir=$DELITE -DStagedSAC.blocks=false TransposeRunner #-Dlms.verbosity=2
echo Execute...
java -Xmx8g -server -cp $DELITE/runtime/target/$VANILLA_VERSION/classes:$SCALA_VANILLA/../lib/scala-library.jar:$SCALA_VANILLA/../lib/scala-compiler.jar -Ddelite.runs=10 ppl.delite.runtime.Delite out.deg
